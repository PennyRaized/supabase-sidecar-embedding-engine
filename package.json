  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/supabase-sidecar-embedding-engine.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/supabase-sidecar-embedding-engine/issues"
  },
  "homepage": "https://github.com/yourusername/supabase-sidecar-embedding-engine#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "supabase": "^1.100.0"
  },
  "files": [
    "src/",
    "docs/",
    "ENVIRONMENT.md",
    "README.md",
    "CONTRIBUTING.md"
  ]
